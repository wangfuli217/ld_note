VAR =
IS_EMPTY = $(if $(VAR), $(info not empty), $(info empty))

ifeq ($(uname),"Darwin")
  logcheck1=find . -name '*.log*' -type f -print0 | xargs -0 grep -inH -e 'error' | wc -l | grep 0
else
  logcheck1=find . -name '*.log*' -type f -print0 | xargs -0 grep -inH -e 'error' | wc -l | grep 0
endif

.PHONY: all

all:
	@echo $(IS_EMPTY)
	@for f in `find . -iname "make_p*"`; do\
		echo $$f;\
	done
