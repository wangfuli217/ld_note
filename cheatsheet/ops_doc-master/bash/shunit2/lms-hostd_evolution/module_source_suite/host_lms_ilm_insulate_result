  assertContains "$RTUCLIENT ilm-result-get $ILM_E_SLOT 8 @output:${output}" "${output}" "${ILM_RES49}"
  assertNotContains "$RTUCLIENT ilm-result-get $ILM_E_SLOT 8 @output:${output}" "${output}" "error"
}

