digraph {

    rankdir = LR;

    node [shape = record];

    table [label = " <head> 哈希表 | <0> 0 | <1> 1 | <2> 2 | <3> 3 "];

    kv1 [label = "<head> 键值对 | { k1 | v1 } "];
    kv2 [label = "<head> 键值对 | { k2 | v2 } "];
    kv3 [label = "<head> 键值对 | { k3 | v3 } "];
    
    table:0 -> kv2:head;
    table:2 -> kv1:head;
    table:3 -> kv3:head;

}
