begin new p; new i; label L;
L:
p<-'formal x;
	begin
	out "isn x";
	out isn x;
	out "isb x";
	out isb x;
	out "isr x";
	out isr x;
	out "isl x";
	out isl x;
	out "isli x";
	out isli x;
	out "isy x";
	out isy x;
	out "isp x";
	out isp x;
	out "isu x";
	out isu x;
	undef
	end';
out "x<-1;";
p(1);
out "x<-true;";
p(true);
out "x<-@i;";
p(@i);
out "x<-L;";
p(L);
out "x<-();";
p(());
out "x<-symbol;";
p("A");
out "x<-'1';";
p('1');
out "x<-undef;";
p(undef);
out "done"
end
