#SRC: JCON

# lexical comparison test

procedure main()
   local s, t

   write("    s1    s2    <<   <<=    ==   ~==   >>=    >>")

   every (s := "" | "a" | "b" | 'c' | "x" | 2 | '') &
	   (t := "" | "a" | "c" | 'x' | '2') do {
      wr(s)
      wr(t)
      wr(s << t  | &null)
      wr(s <<= t | &null)
      wr(s == t  | &null)
      wr(s ~== t | &null)
      wr(s >>= t | &null)
      wr(s >> t  | &null)
      write()
      }
   end

procedure wr(s)
   writes(right(image(\s) | "---", 6))
   return
end
