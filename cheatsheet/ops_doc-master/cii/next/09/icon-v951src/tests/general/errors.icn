procedure p7()
   write("(2 * \"a\") | monitor(&line) ----> ",
      image((2 * "a") | monitor(&line)) | "none")
   write("(2 / \"a\") | monitor(&line) ----> ",
      image((2 / "a") | monitor(&line)) | "none")
