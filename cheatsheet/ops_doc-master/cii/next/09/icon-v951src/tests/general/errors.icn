procedure p8()
   write("(x :=: \"a\") | monitor(&line) ----> ",
      image((x :=: "a") | monitor(&line)) | "none")
   write("(x <-> \"b\") | monitor(&line) ----> ",
      image((x <-> "b") | monitor(&line)) | "none")
   write("((x & 2 & 3 & 4) := 3) | monitor(&line) ----> ",
