      image((2 < "a") | monitor(&line)) | "none")
   write("(0 > &null) | monitor(&line) ----> ",
      image((0 > &null) | monitor(&line)) | "none")
   write("(2 <= \"a\") | monitor(&line) ----> ",
      image((2 <= "a") | monitor(&line)) | "none")
