--__call元方法，可以将表作为函数使用
mytable6={"Lua","C#","Python","Go"}
mymetatable6={
__call=function(tab,arg)
	print(arg)
	return arg
end
}
mytable6=setmetatable(mytable6,mymetatable6)
print(mytable6("Java"))

table1=setmetatable({key1=1,key2=2,key3=3},{__call=
function(t,value)
	for i,v in pairs(t) do
		t[i]=t[i]+value
	end
	return t
end
})
table1(2)     -- 把表像函数一样使用
for i,v in pairs(table1) do
    print(i,v)
end