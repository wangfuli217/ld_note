LUA_USE_APICHECK	main.c	3;"	d	file:
LUA_USE_APICHECK	main.cpp	3;"	d	file:
lerror	main.c	/^void lerror(lua_State *L, char *msg) {$/;"	f
lerror	main.cpp	/^void lerror(lua_State *L, char *msg) {$/;"	f
main	main.c	/^int main(int argc, char** argv)$/;"	f
main	main.cpp	/^int main(int argc, char** argv){$/;"	f
main	stackdump.c	/^int main() {$/;"	f
main	stephen_main.cpp	/^int main(void)$/;"	f
stackdump	stackdump.c	/^static void stackdump(lua_State *L) {$/;"	f	file:
