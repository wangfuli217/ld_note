t = {}
t.foo = 123 -- same as t["foo"] (but not t[foo], which would use the variable foo as the key)
print(t.foo)
print(t["foo"])

t = {["foo"] = "bar", [123] = 456}
print(t.foo)
print(t["foo"])

t = {foo = "bar"} -- same as ["foo"]="bar" (but not [foo]="bar" , that would use the variable foo)
print(t.foo)
print(t["foo"])
